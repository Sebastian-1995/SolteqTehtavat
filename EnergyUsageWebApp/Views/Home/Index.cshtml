@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
@model IEnumerable<monthlyData>

@{ Layout = "_Layout"; ViewBag.Title = "Task for Solteq";}
 
<h2>Monthly Data</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>Month</th>
            <th>Value</th>
            <th>Unit</th>
        </tr>
    </thead>
    <tbody>
        
        @foreach (var r in Model)
        {
        
        <tr>   
            <td>@r.Month.ToString("MMM")</td>
            <td>@r.Total</td>
            <td>kWh</td>
        </tr>

        }

    </tbody>
</table>
</div>